<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox fx:id="assessmentCard" alignment="CENTER_LEFT" minHeight="105" prefWidth="350">
        <padding>
            <Insets bottom="5" left="15" right="5" top="5" />
        </padding>
        <HBox alignment="CENTER_LEFT" spacing="20" prefWidth="350">
            <Label fx:id="id" />
            <GridPane alignment="CENTER">
                <columnConstraints>
                    <ColumnConstraints hgrow="ALWAYS" percentWidth="90.0" />
                    <ColumnConstraints hgrow="ALWAYS" percentWidth="10.0" />
                </columnConstraints>
                <children>
                    <VBox alignment="CENTER_LEFT" GridPane.columnIndex="0" HBox.hgrow="ALWAYS">
                        <Label fx:id="name" styleClass="cell_big_label" text="\$first" wrapText="true" />
                        <Label fx:id="grade" styleClass="cell_small_label" text="\$grade" wrapText="true" />
                    </VBox>
                    <HBox alignment="CENTER_RIGHT" GridPane.columnIndex="1">
                        <Label fx:id="percentage" prefHeight="18.0" prefWidth="197.0" text="\$percentage" HBox.hgrow="ALWAYS" />
                    </HBox>
                </children>
               <rowConstraints>
                  <RowConstraints />
               </rowConstraints>
            </GridPane>
        </HBox>
    </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
    </GridPane>
</HBox>
